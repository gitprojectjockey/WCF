<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
  </startup>
  <system.serviceModel>
    <bindings>
      <wsHttpBinding>
        <binding name="serviceEndPoint">
          <security>
            <message clientCredentialType="UserName" />
          </security>
        </binding>
      </wsHttpBinding>
    </bindings>
    <behaviors>
      <endpointBehaviors>
        <behavior name="secure">
          <clientCredentials>
            <serviceCertificate>
              <authentication certificateValidationMode="ChainTrust"/>
            </serviceCertificate>
          </clientCredentials>
        </behavior>
      </endpointBehaviors>
    </behaviors>
    <client>
      <endpoint address="http://localhost/SouthWestLumberYard/ToothPickService.svc"
        binding="wsHttpBinding" bindingConfiguration="serviceEndPoint"
        contract="ToothPickService.IToothPickService" name="serviceEndPoint">
        <identity>
          <certificate encodedValue="" />
        </identity>
      </endpoint>
    </client>
  </system.serviceModel>
</configuration>